{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "idea-explorer",
	"main": "src/index.ts",
	"compatibility_date": "2025-05-06",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"vars": {
		"GITHUB_REPO": "nbbaier/idea-explorer",
		"GITHUB_BRANCH": "main"
	},
	"containers": [
		{
			"class_name": "Sandbox",
			"image": "./Dockerfile",
			"instance_type": "basic"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "Sandbox",
				"name": "Sandbox"
			}
		]
	},
	"migrations": [
		{
			"new_sqlite_classes": ["Sandbox"],
			"tag": "v1"
		}
	],
	"kv_namespaces": [
		{
			"binding": "JOBS",
			"preview_id": "placeholder_preview_id",
			"id": "placeholder_production_id"
		}
	]
}
